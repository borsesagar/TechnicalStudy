C = gcc
CFLAGS = -g -o
SERVERTARGET = server
CLIENTTARGET = client
RM = rm -rf


all: $(SERVERTARGET) $(CLIENTTARGET)

$(SERVERTARGET) : $(SERVERTARGET).c
	$(CC) $(CFLAGS) $(SERVERTARGET) $(SERVERTARGET).c

$(CLIENTTARGET) : $(CLIENTTARGET).c
	$(CC) $(CFLAGS) $(CLIENTTARGET) $(CLIENTTARGET).c
clean:
	$(RM) $(SERVERTARGET) $(CLIENTTARGET) 
