CC = gcc
CFLAGS = -g -o
SENDTARGET = Server
RECEIVETARGET = Client
RM = rm -rf


all: $(SENDTARGET) $(RECEIVETARGET)

$(SENDTARGET) : $(SENDTARGET).c
	$(CC) $(CFLAGS) $(SENDTARGET) $(SENDTARGET).c

$(RECEIVETARGET) : $(RECEIVETARGET).c
	$(CC) $(CFLAGS) $(RECEIVETARGET) $(RECEIVETARGET).c
clean:
	$(RM) $(SENDTARGET) $(RECEIVETARGET) 	
